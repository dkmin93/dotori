package day03;

public class Person {
	
	//this() - 생성자 연결
	public String name;
	public int age;
	public int ssn;
	
	
	public Person(String name, int age, int ssn) {
		this.name = name.toUpperCase();
		this.age = age + 1;
		this.ssn = ssn;
		
	}
	
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
		this.ssn = 0;
	}
	

}
